{{!-- beersFromApi key features for beer cards
image_url:
name:
tagline:
description:
Features for beer present
image_url: (on the left by itself)
name: 
tagline: (include first_brewed: on this line)
description:
food_pairing: (is an array)
brewers_tips:
 --}}
<div>
    {{#each test}}
        <div><img src="{{image_url}}" alt="{{name}} image"></div>
        <div>
            <h1>{{name}}</h1>
            <h2>{{tagline}}</h2>
            <p>{{description}}</p>
            <h3>Food Pairings</h3>
            <ul>
                {{#each food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <p><b>Brewers Tips: </b> {{brewers_tips}}</p>
        </div>
    {{/each}}
</div>